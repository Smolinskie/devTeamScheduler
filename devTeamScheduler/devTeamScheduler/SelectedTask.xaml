        <TextBox HorizontalAlignment="Left" Height="23" Margin="337,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="205" Margin="199,94,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="412"/>
        <Label Content="Task Name:" HorizontalAlignment="Left" Margin="262,66,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.367,-0.395"/>
