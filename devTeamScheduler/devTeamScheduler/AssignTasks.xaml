        <ComboBox Name="UserDropDown" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="335,82,0,0">
        </ComboBox>
        <Label Content="User:" HorizontalAlignment="Left" Margin="254,78,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.032,0.344" Width="70"/>
        <Label Content="Task Name:" HorizontalAlignment="Left" Margin="254,12,0,0" VerticalAlignment="Top"/>
        <Label Content="Dev Branch:" HorizontalAlignment="Left" Margin="254,43,0,0" VerticalAlignment="Top"/>
        <TextBox Name="DevBranchTextBox" HorizontalAlignment="Left" Height="23" Margin="332,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="148,202,0,0" VerticalAlignment="Top" Width="76" Click="Button_Click_2"/>

    </Grid>
</Window>
