        xmlns:local="clr-namespace:devTeamScheduler"
        mc:Ignorable="d"
        Title="Task_assigner" Height="504.848" Width="846.122" Background="AliceBlue">
    <Grid AutomationProperties.HelpText="Tasks description">
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="176,430,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.932,0.566" Click="Button_Click"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="380,430,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.533,2.158" Click="Button_Click_1"/>
        <TextBox Name="TaskDescriptionTextBox" HorizontalAlignment="Left" Height="213" Margin="260,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="281" RenderTransformOrigin="-0.225,-1.873" Text="tasks description"/>
        <DatePicker Name="AssignDueDate" HorizontalAlignment="Left" Margin="369,342,0,0" VerticalAlignment="Top"/>
